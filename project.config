[receive]
	requireChangeId = true
[access "refs/*"]
	read = group TYPO3v4/Extensions/media-Leaders
	create = group TYPO3v4/Extensions/media-Leaders
	push = group TYPO3v4/Extensions/media-Leaders
[access "refs/for/refs/*"]
	push = group TYPO3v4/Extensions/media-Leaders
	pushMerge = group TYPO3v4/Extensions/media-Leaders
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group TYPO3v4/Extensions/media-Leaders
	label-Verified = -1..+2 group TYPO3v4/Extensions/media-Leaders
	submit = group TYPO3v4/Extensions/media-Members
	create = group TYPO3v4/Extensions/media-Leaders
	owner = group TYPO3v4/Extensions/media-Leaders
	label-Code-Review = -2..+2 group TYPO3v4/Extensions/media-Members
	label-Verified = -1..+2 group TYPO3v4/Extensions/media-Members
[access "refs/tags/*"]
	pushTag = group TYPO3v4/Extensions/media-Leaders
	create = group TYPO3v4/Extensions/media-Leaders
[submit]
	mergeContent = true
	action = cherry pick
[project]
	state = active
[access "refs/for/refs/heads/*"]
	pushMerge = group TYPO3v4/Extensions/media-Members
	forgeCommitterIdentity = group TYPO3v4/Extensions/media-Leaders
